                    },
                    {
                        "name": "zip",
                        "in": "formData",
                        "description": "Five digit integer zip code of the account owner",
                        "required": false,
                        "type": "integer"
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Organization created",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "status": {
                                    "type": "string",
                                    "description": "success or fail"
                                },
                                "data": {
                                    "type": "string",
                                    "description": "A JSON representation containing one organization and one member."
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Invalid credentials",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "status": {
                                    "type": "string",
                                    "description": "fail"
                                },
                                "message": {
                                    "type": "string",
                                    "description": "Invalid credentials"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Required fields were missing",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "status": {
                                    "type": "string",
                                    "description": "fail"
                                },
                                "message": {
                                    "type": "string",
                                    "description": "Missing required field: {field name}"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "A request method other than post was used.",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "status": {
                                    "type": "string",
                                    "description": "fail"
                                },
                                "message": {
                                    "type": "string",
                                    "description": "A request method other than post was used."
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid data was provided for at least one of the fields",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "status": {
                                    "type": "string",
                                    "description": "fail"
                                },
                                "message": {
                                    "type": "string",
                                    "description": "Validation errors occurred"
                                },
                                "errors": {
                                    "type": "string",
                                    "description": "An object of validation errors, indexed by field name"
                                }
                            }
                        }
                    },
                    "429": {
                        "description": "The API rate limit of 300 members per day has been exceeded.",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "status": {
                                    "type": "string",
                                    "description": "fail"
                                },
                                "message": {
                                    "type": "string",
                                    "description": "The API rate limit of 300 members per day has been exceeded."
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "There was an error saving the member or organization.",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "status": {
                                    "type": "string",
                                    "description": "fail"
                                },
                                "message": {
                                    "type": "string",
                                    "description": "There was an error saving the member or organization."
                                }
                            }
                        }
                    }
                }
            }
        },






        "/api_organizations/cancel": {
            "post": {
                "summary": "Deactivates an organization, blocking all login attempts from members to this organization.",
                "operationId": "createOrganization",
                "tags": [
                    "organizations"
                ],
                "parameters": [
                    {
                        "name": "api_key",
                        "in": "formData",
                        "description": "YouthPros API key",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "api_secret",
                        "in": "formData",
                        "description": "YouthPros API secret",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "organization_id",
                        "in": "formData",
                        "description": "The id of the organization",
                        "required": true,
                        "type": "integer"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "The organization was succesffully deactivated.",
                        "schema": {
